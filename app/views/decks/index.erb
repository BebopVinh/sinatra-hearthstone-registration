<h1>All decks</h1>
<h3><a href="/players/"></a></h3>
<% Deck.all.each do |deck| %>
   <li>
      <span>
      <a href="/decks/<%=deck.id%>">
      <%= deck.name + " #{deck.profession.name} (#{deck.archetype})"%>
      </a>
      created by:
      <a href="/players/<%=deck.creator.slug%>/decks">
      <%= deck.creator.username %>
      </a>
      </span>
      </li><br>
<% end %>